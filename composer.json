{
    "name": "Ghwolfzx/kuaishou-marketing-sdk",
    "description": "Kuaishou MarketingAPI SDK",
    "keywords": [
        "kuaishou",
        "marketing",
        "sdk"
    ] ,
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ghwolf",
            "email": "ghwolf@foxmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CloudyCity\\KuaishouMarketingSDK\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    }
}
